<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.GuiAppController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <Label text="Energy Community Dashboard" />
    <Button onAction="#loadCurrentData" text="Lade aktuelle Daten V2" />
    <Button onAction="#loadHistoricalData" text="Lade historische Daten" />
    <TextArea fx:id="outputArea" prefHeight="200" prefWidth="400" />
</VBox>
